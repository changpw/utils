<?xml version="1.0" encoding="UTF-8"?>
<package name="stm" namespace="passwordCardManage" extends="filter-base.xml,filter-serial.xml">

	<!-- 双挂密码挂失 -->
	<service name="devpasswordLoss" class="service.stm.cardManage.PasswordCardManage" method="passwordLoss"/>
	
	<!-- 双挂密码重置 -->
	<service name="devpasswordReset" class="service.stm.cardManage.PasswordCardManage" method="passwordReset"/>
	
	<!-- 双挂卡挂失 -->
	<service name="devcardLost" class="service.stm.cardManage.PasswordCardManage" method="cardLost"/>
	
	<!-- 双挂换卡 -->
	<service name="devcardChange" class="service.stm.cardManage.PasswordCardManage" method="cardChange"/>
	
	<!-- 客户收取手续费账号列表 -->
	<service name="devfindLostAcctQuery" class="service.stm.cardManage.PasswordCardManage" method="findLostAcctQuery"/>
	
	<!-- 双挂写卡脚本通知 -->
	<service name="devwriteCardNotice" class="service.stm.cardManage.PasswordCardManage" method="writeCardNotice">
	</service>
	
	<!-- 双挂第一次指纹授权记录授权员 -->
	<service name="devfingerprintImpower" class="service.stm.cardManage.PasswordCardManage" method="fingerprintImpower"/>
</package>