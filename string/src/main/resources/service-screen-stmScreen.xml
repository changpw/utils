	<service name="devaddScreenDevice" class="service.manage.screenDevManage.deviceInfo.ScreenDevManage" method="addScreenDevice">
	</service>
	<!--删除大屏设备 -->
	<service name="devdelScreenDevice" class="service.manage.screenDevManage.deviceInfo.ScreenDevManage" method="delScreenDevice">
	</service>
	<!--修改大屏设备 -->
	<service name="devmodifyScreenDevice" class="service.manage.screenDevManage.deviceInfo.ScreenDevManage" method="modifyScreenDevice">
	</service>
	<!--增加节目配置 -->
	<service name="devaddProgramConfig" class="service.manage.screenDevManage.deviceInfo.ProgramConfig" method="addProgramConfig">
	</service>
	<!--删除节目配置 -->
	<service name="devdelProgramConfig" class="service.manage.screenDevManage.deviceInfo.ProgramConfig" method="delProgramConfig">
	</service>
	<!--修改节目配置 -->
	<service name="devmodifyProgramConfig" class="service.manage.screenDevManage.deviceInfo.ProgramConfig" method="modifyProgramConfig">
	</service>
	
